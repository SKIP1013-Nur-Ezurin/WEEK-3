package numericData;
public class Exercise1 { //Exercise1 ialah nama jaca class

	public static void main(String[] args) { //Dalam class mesti ada main
		// TODO Auto-generated method stub
		
		//Declares integer variables and assign a value
		int number1 = 11; 
		int number2 = 22;
		int number3 = 33;
		int number4 = 44;
		int number5 = 55;
		double number6 = 66;
		double number7 = 88;
		float number8 = 99;
		float number9 =200;
		
		//declares an integer to sum
		int sum; 
		int avg;
		double div;
		float div2;
		double mul;
		float mul2;
		
		//calculate
		sum = number1 + number2 + number3 + number4 + number5;//compute string
		avg = sum/5;
		div = number7/number6;
		div2 = number9/number8;
		mul = number1*number9*number5;
		mul2 = (number1 * number2)/ number3;
		
		//print the value
		System.out.print("The sum is ");
		System.out.println(sum);
		System.out.print("The averages is ");
		System.out.println(avg);
		System.out.println();
		System.out.print("The divide number is: " +div);
		System.out.println();
		System.out.printf("Divide float :%.2f ", div2);
		System.out.println();
		System.out.print("the multiply of the number is: " +mul);
		System.out.println();
		System.out.printf("the multiply number is:%.2f", mul);
		System.out.println();
		System.out.printf("the answer is:%.2f", mul2);
		
		

	}

}
